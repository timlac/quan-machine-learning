# MAIN ENV

(nohup python -m src.learning.elastic_net files/out/video/video_data_intensity_train.csv files/out/video/ ;

nohup python -m src.learning.knn files/out/video/video_data_intensity_train.csv files/out/video/ ;

nohup python -m src.learning.decision_tree files/out/video/video_data_intensity_train.csv files/out/video/ ;

nohup python -m src.learning.random_forrest files/out/video/video_data_intensity_train.csv files/out/video/) > files/out/video/parameter_search.log 2>&1 &




# TEST ENV

